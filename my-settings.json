{
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    "debug.console.fontFamily": "Operator Mono, Inconsolata for Powerline",
    "files.autoSave": "onWindowChange",
    "files.trimTrailingWhitespace": true,
    "workbench.iconTheme": "file-icons",
    "editor.formatOnType": true,
    "editor.tabSize": 2,
    "editor.fontWeight": "400",
    "editor.renderWhitespace": "all",
    "editor.letterSpacing": 0.5,
    "editor.fontLigatures": true,
    "editor.fontFamily": "JetBrains Mono, Cascadia Code ,Fira Code, IBM Plex Mono , Monoid, Dank Mono, Operator Mono, Inconsolata for Powerlinefo",
    "explorer.sortOrder": "type",
    "terminal.integrated.fontFamily": "Operator Mono, Inconsolata for Powerline",
    "terminal.integrated.fontWeight": "400",
    "terminal.integrated.fontSize": 13,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.autofetch": true,
    "terminal.integrated.rendererType": "dom",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "terminal.external.osxExec": "iTerm.app",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorWidth": 2,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Bio Dark",
    "editor.lineHeight": 29,
    "editor.fontSize": 13,
}
