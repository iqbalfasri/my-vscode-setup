{
    "editor.fontLigatures": true,
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "editor.wordWrap": "on",
    "editor.letterSpacing": 0.5,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.fontFamily": "Operator Mono, Inconsolata for Powerline",
    "workbench.startupEditor": "newUntitledFile",
    "terminal.integrated.rendererType": "dom",
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    "editor.cursorWidth": 5,
    "workbench.iconTheme": "file-icons",
    "explorer.confirmDragAndDrop": false,
    "editor.tabSize": 2,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontFamily": "Operator Mono, Inconsolata for Powerline",
    "editor.fontSize": 14,
    "editor.lineHeight": 30,
    "editor.formatOnType": true,
    "editor.renderWhitespace": "boundary",
    "window.zoomLevel": 1,
    "terminal.integrated.fontSize": 14,
    "workbench.colorTheme": "Cobalt Next",
    "debug.console.fontFamily": "Operator Mono, Inconsolata for Powerline",
    "workbench.panel.defaultLocation": "right",
}
